{
  "manifest_version": 2,
  "name": "Content script example",
  "description": "Trial of content_scripts",
  "version": "1.0",
  "permissions": [
    "tabs",
    "https://www.google.co.jp/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.google.co.jp/*"],
      "js": ["content_script.js"],
      "run_at": "document_end"
    }
  ]
}